/*
  Blink
  Turns on an LED on for one second, then off for one second, repeatedly.
 
  This example code is in the public domain.
 */
unsigned int Timer;

void init()
{
	Timer = getTimeMs();
}

void program()
{
	if(getElapsedTimeMs(Timer) > 1000)
	{
		Timer = getTimeMs();
		togglePin(LED1);		// toggle LED on/off
	}
}
